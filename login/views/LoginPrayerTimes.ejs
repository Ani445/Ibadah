
    <div class="salat-time">
		<style type="text/css">
			.tg {
				border-collapse: collapse;
				border-spacing: 0;
			}

			.tg td {
				border-color: black;
				border-style: solid;
				border-width: 1px;
				font-family: Arial, sans-serif;
				font-size: 14px;
				overflow: hidden;
				padding: 10px 5px;
				word-break: normal;
			}

			.tg th {
				border-color: black;
				border-style: solid;
				border-width: 2px;
				font-family: Arial, sans-serif;
				font-size: 14px;
				font-weight: normal;
				overflow: hidden;
				padding: 10px 5px;
				word-break: normal;
                font-weight: bold;
			}

			.tg .tg-0lax {
				text-align: center;
				vertical-align: top;
				background-color: transparent;
			}
		</style>
		<table class="tg">
			<tr>
				<th class="tg-0lax">Salah</th>
				
				<th class="tg-0lax">Time to start</th>
			</tr>
			<tr>
				<td class="tg-0lax">Fazr</td>
				
				<td class="tg-0lax"><%= data.Fazr %></td>
			</tr>
			<tr>
				<td class="tg-0lax">Sunrise</td>
				
				<td class="tg-0lax"><%= data.Sunrise %></td>
			</tr>
			<tr>
				<td class="tg-0lax">Zuhr</td>
				
				<td class="tg-0lax"><%= data.Zuhr %></td>
			</tr>
			<tr>
				<td class="tg-0lax">Asr</td>
			
				<td class="tg-0lax"><%= data.Asr %></td>
			</tr>
			<tr>
				<td class="tg-0lax">Magrib</td>
				
				<td class="tg-0lax"><%= data.Maghrib %></td>
			</tr>
			<tr>
				<td class="tg-0lax">Isha</td>
				
				<td class="tg-0lax"><%= data.Isha %></td>
			</tr>
		</table>
	</div>
